{
  "name": "nextjs15-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --write ./src",
    "lint:js": "next lint",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:fix": "yarn prettier && eslint --fix \"src/**/*.+(js|jsx|ts|tsx)\" && stylelint --fix \"**/*.scss\"",
    "type-css": "yarn typed-scss-modules src/ --exportType default --logLevel silent --watch",
    "dev": "yarn type-css & yarn lint && next dev --turbopack",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "framer-motion": "^12.17.0",
    "googleapis": "^150.0.1",
    "lightningcss": "^1.22.0",
    "lucide-react": "^0.514.0",
    "next": "^15.3.3",
    "postcss": "^8.5.5",
    "postcss-loader": "^8.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.83.4",
    "sharp": "^0.33.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "prettier": "^3.4.2",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-order": "^6.0.4",
    "typed-scss-modules": "^8.1.1",
    "typescript": "^5"
  }
}
